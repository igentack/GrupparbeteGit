
@{
    ViewData["Title"] = "Seed";
}

<h1>Seed @ViewData["AddMembers"] </h1>

<form asp-action="Seed" method="get">
    <label for="AddMembers">Add members</label>
    <input type="text" id="AddMembers" name="AddMembers">
    <input type="submit" value="Submit">
</form>
<form asp-action="Seed" method="get">
    <label for="AddParkingSpots">Add Parking spots</label>
    <input type="text" id="AddParkingSpots" name="AddParkingSpots">
    <input type="submit" value="Submit">
</form>
<form asp-action="Seed" method="get">
    <label for="AddVehicles">Add vehicles(there has to be at least one member and one vehicle type)</label>
    <input type="text" id="AddVehicles" name="AddVehicles">
    <input type="submit" value="Submit">
</form>




